<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>MyCouch.AspNet.Identity</id>
    <title>MyCouch.AspNet.Identity - an ASP.Net identity provider for CouchDb and Cloudant</title>
    <version>$version$</version>
    <authors>danielwertheim</authors>
    <copyright>Copyright 2014 Daniel Wertheim</copyright>
    <summary>An ASP.Net identity provider for CouchDb and Cloudant.</summary>
